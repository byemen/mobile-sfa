    		setTitSendInfo("request", "", "", "mdms/mp/main:S310001_W01_S03");
    		setSubmissison("submissionDlrNotice");
        }
        /****************************************************************
		* 15일 이내 해당 개월 수 출고 도래 예정
		* @param  : 없음
		* @return : 없음
		******************************************************************/ 
        function fn_SearchCarOut(){
        	setTitSendInfo("request", "", "", "mdms/mp/main:S310001_W01_S04");
        	setInputParamData("GROUP_ID",GV_UP_GROUP_ID);
        	setInputParamData("MNG_SC_ID",GV_USER_ID);
        	setInputParamData("RESV_DT", gfn_GetDate(0,0,0).replaceAll("-",""));    
    		setSubmissison("submissionCarOut");
    		
    		setTitSendInfo("request", "", "", "mdms/mp/main:S310001_W01_S05");
    		setInputParamData("GROUP_ID",GV_UP_GROUP_ID);
        	setInputParamData("MNG_SC_ID",GV_USER_ID);
        	setInputParamData("RESV_DT", gfn_GetDate(0,0,0).replaceAll("-",""));
    		setSubmissison("submissionCustList");
        }
        /****************************************************************
		* 금일 영업 기회 조회
		* @param  : 없음
		* @return : 없음
		******************************************************************/ 
        function fn_SearchActive(){
        
        	setTitSendInfo("request", "", "", "mdms/mp/main:S310001_W01_S06");
        	setInputParamData("DEALER_ID",GV_UP_GROUP_ID);
        	setInputParamData("MNG_SC_ID", GV_USER_ID);     	
        	
    		setSubmissison("submissionActive");        
        }
        /****************************************************************
		* 출고 고객 리스트 팝업
		* @param  : 없음
		* @return : 없음
		******************************************************************/ 
        function fn_CarOutCust(row,col){        
        	if(col < 1)	return;
        	
        	var sMon = "";
        	if(col == 1){
				sMon = "1";
			}else if(col == 2){
				sMon = "3";
			}else if(col == 3){
				sMon = "6";
			}else if(col == 4){
				sMon = "12";
		    }else if(col == 5){
				sMon = "18";
			}else if(col == 6){
				sMon = "24";
			}else if(col == 7){
				sMon = "36";
			}else if(col == 8){
				sMon = "48";
			}
        	
        	WebSquare.ModelUtil.setInstanceValue("params/param[@id='GROUP_ID']",GV_UP_GROUP_ID);
        	WebSquare.ModelUtil.setInstanceValue("params/param[@id='MNG_SC_ID']",GV_USER_ID);
        	WebSquare.ModelUtil.setInstanceValue("params/param[@id='RESV_DT']",gfn_GetDate(0,0,0).replaceAll("-",""));
        	WebSquare.ModelUtil.setInstanceValue("params/param[@id='MON']",sMon);
        	
        	gfn_OpenPopup("/mp/main/S310001_P01.xml","출고 후 도래 예정",905,489,44,46);
        }
        /****************************************************************
		* 팝업 닫기
		* @param  : 없음
		* @return : 없음
		******************************************************************/
        function fn_PopupClose(){ 
        	gfn_PopupClose();
        }
        /****************************************************************
		* 공지사항 팝업
		* @param  : 없음
		* @return : 없음
		******************************************************************/
        function fn_Notice(seq){
        	FV_sSeq = seq;

        	gfn_OpenPopup("/mp/comm/Notice.xml","공지사항",694,600,100,10);
        }
        function fn_IsLoading(){
        	if(FV_nCnt == 6)
        		parent.fn_SpinnerHide();
        }

        fn_OnLoad();
