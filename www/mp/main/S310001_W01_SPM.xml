     	/****************************************************************
		* 화면로드
		* @param  : 없음
		* @return : 없음
		******************************************************************/
        function fn_OnLoad(){
        	var curDate = new Date();
		 	var dHour = curDate.getHours();
		 	var dMin = curDate.getMinutes();			
		 	if(dHour < 10) dHour = "0" + dHour;
		 	if(dMin < 10) dMin = "0" + dMin;
		 	
        	st_FrDt.setValue(gfn_GetDate(0,0,0,"FM"));
        	st_ToDt.setValue(gfn_GetDate(0,0,0));
        	st_Time.setValue(dHour + ":" + dMin);
        	
        	fn_SearchDaily("C");
        	fn_SearchNotice();        	
        	fn_SearchCarOut();
        	fn_SearchActive();
        	fn_SearchSpmAct(); // SPM 활동현황
        }
        /****************************************************************
		* 계약/판매 실적 조회
		* @param  : gb
		* @return : 없음
		******************************************************************/ 
        function fn_SearchDaily(gb){
        	var sUserId = "";
			var sDeptCd = "";
			
			if(gb == "M"){
				sUserId = GV_USER_ID;
			} else if(gb == "D"){
				sDeptCd = GV_DEPT_CD;
			}

        	setTitSendInfo("request", "", "", "mdms/mp/main:S310001_W01_S01");
        	setInputParamData("DEALER_ID",GV_UP_GROUP_ID);
        	setInputParamData("USER_ID", sUserId);
        	setInputParamData("DEPT_CD", sDeptCd);
        	setInputParamData("MNG_SC_ID", GV_USER_ID);
        	setInputParamData("FR8_DATE", st_FrDt.getValue().replaceAll("-",""));
        	setInputParamData("FR10_DATE", st_FrDt.getValue()); 
        	setInputParamData("TO10_DATE", st_ToDt.getValue());
        	setInputParamData("TO10_DATE31", gfn_GetDate(0,0,0,"TM")); 
        	setInputParamData("TARGET_DAY", st_ToDt.getValue().substring(8,10));       	

    		setSubmissison("submissionTrend");        
        }
        /****************************************************************
		* SPM 활동현황
		* @param  : 없음
		* @return : 없음
		******************************************************************/ 
        function fn_SearchSpmAct(){
        	setTitSendInfo("request", "", "", "mdms/mp/main:S310001_W01_S07");
        	setInputParamData("MNG_SC_ID",GV_USER_ID);

    		setSubmissison("submissionSpm");        
        }
