		<script type="javascript"><![CDATA[
        	
        	var setRow = function(idx, node) {
        	
				generator1.insert();
			
        		var numberValue = WebSquare.xml.getValue(node, "number");
        		var nameValue = WebSquare.xml.getValue(node, "name");
        		var addressValue = WebSquare.xml.getValue(node, "address");
        		
				var numberObj = generator1.getChild(idx, "numberOutput");
				var nameObj = generator1.getChild(idx, "nameOutput");
				var addressObj = generator1.getChild(idx, "addressOutput");
				var groupObj = generator1.getChild(idx, "group1");
				
				numberObj.setValue(numberValue);
				nameObj.setValue(nameValue);
				addressObj.setValue(addressValue);
				
				if((numberValue % 2) != 0) {
					groupObj.addClass('line1')
				} else  {
					groupObj.addClass('line2')
				}
				
			};
			
			var nodeArr = WebSquare.ModelUtil.findInstanceNodes("response/list/data");
			
			for(var i=0;i<nodeArr.length;i++) {
				setRow(i, nodeArr[i]);
			}
			
         ]]></script>
